    {
      "keys": "p.a",
      "name": "Go to alternate file if exists",
      "type": "command",
      "command": "alternate.alternateFile"
    },
    {
      "keys": "p.A",
      "name": "Create alternate file if doesn't exists",
      "type": "command",
      "command": "alternate.createAlternateFile"
    }
  ],
